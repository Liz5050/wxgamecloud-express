{
  "name": "wxcloudrun-express",
  "version": "1.0.0",
  "description": "微信云托管 express 框架模版",
  "main": "index.js",
  "scripts": {
    "start": "node --expose-gc index.js",
    "dev": "node --expose-gc -r dotenv/config index.js dotenv_config_path=.env.local",
    "dev:80": "set PORT=80&& node --expose-gc -r dotenv/config index.js dotenv_config_path=.env.local",
    "dev:3000": "set PORT=3000&& node --expose-gc -r dotenv/config index.js dotenv_config_path=.env.local",
    "dev:custom": "node --expose-gc -r dotenv/config index.js",
    "test": "node tests/integration/testOptimization.js",
    "test:safe": "node tests/integration/start-test.js",
    "test:quick": "node tests/unit/quick-test.js",
    "test:perf": "node -e \"const { OptimizationTester } = require('./tests/integration/testOptimization'); new OptimizationTester().runAllTests();\"",
    "test:monitor": "node -e \"const { PerformanceMonitor } = require('./src/services/PerformanceMonitor'); const monitor = new PerformanceMonitor(); setInterval(() => console.log(JSON.stringify(monitor.getPerformanceReport(), null, 2)), 3000);\"",
    "prod": "set NODE_ENV=production&& node index.js",
    "version:major": "node scripts/version-manager.js major",
    "version:minor": "node scripts/version-manager.js minor",
    "version:patch": "node scripts/version-manager.js patch",
    "version:log": "node scripts/version-manager.js log",
    "version:current": "node scripts/version-manager.js current"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^4.16.4",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.2",
    "sequelize": "^6.8.0"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "devDependencies": {
    "axios": "^1.12.1"
  }
}
