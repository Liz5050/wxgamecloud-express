{
  "name": "wxcloudrun-express",
  "version": "1.0.0",
  "description": "微信云托管 express 框架模版",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "node testOptimization.js",
    "test:safe": "node start-test.js",
    "test:quick": "node quick-test.js",
    "test:perf": "node -e \"const { OptimizationTester } = require('./testOptimization'); new OptimizationTester().runAllTests();\"",
    "test:monitor": "node -e \"const { PerformanceMonitor } = require('./performanceMonitor'); const monitor = new PerformanceMonitor(); setInterval(() => console.log(JSON.stringify(monitor.getPerformanceStats(), null, 2)), 3000);\"",
    "dev": "set NODE_ENV=development&& node index.js",
    "prod": "set NODE_ENV=production&& node index.js"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^4.16.4",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.2",
    "sequelize": "^6.8.0"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "devDependencies": {
    "axios": "^1.12.1"
  }
}
